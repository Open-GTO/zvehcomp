/*
	Component type functions
*/

stock GetVehicleComponentTypeId(name[])
{
	for (new i = 0; i < sizeof(gComponentType); i++) {
		if (strcmp(gComponentType[i], name, true) == 0) {
			return i;
		}
	}

	return ZVEH_INVALID_COMPONENT_ID;
}

stock GetVehicleComponentTypeName(type, name[], size = sizeof(name))
{
	if (type < 0 || type >= sizeof(gComponentType)) {
		return 0;
	}

	return strunpack(name, gComponentType[type], size);
}

stock GetVehicleCompatibleTypes(model, array[ZVEH_MAX_COMPONENT_TYPES], &array_size)
{
	new
		components[ZVEH_MAX_COMPONENTS],
		components_size;

	new isok = GetVehicleCompatibleUpgrades(model, components, components_size);
	if (!isok) {
		return 0;
	}

	new
		type_id,
		bool:is_type_exist[ZVEH_MAX_COMPONENT_TYPES];

	array_size = 0;

	for (new i = 0; i < components_size; i++) {
		type_id = GetVehicleComponentType(components[i]);

		if (!is_type_exist[type_id]) {
			is_type_exist[type_id] = true;

			array[array_size++] = type_id;
		}
	}

	return 1;
}

/*
	Component upgrade functions
*/

stock GetVehicleComponentName(componentid, cname[], size = sizeof(cname))
{
	new index = componentid - 1000;
	if (!(0 <= index < sizeof(gComponentName))) {
		return 0;
	}

	return strunpack(cname, gComponentName[index], size);
}

stock GetVehicleCompatibleUpgrades(model, array[ZVEH_MAX_COMPONENTS], &array_size)
{
	if (!(400 <= model <= 611)) {
		return 0;
	}

	new
		index;

	index = model - 400;
	array_size = 0;

	for (new i = 0; i < ZVEH_MAX_COMPONENTS; i++) {
		if (gComponents[index]{i} == ZVEH_INVALID_COMPONENT_ID) {
			break;
		}

		array[array_size++] = gComponents[index]{i} + 1000;
	}

	return 1;
}

stock IsVehicleUpgradeCompatible(model, componentid)
{
	if (!(400 <= model <= 611)) {
		return 0;
	}

	new
		index,
		component_value;

	index = model - 400;
	component_value = componentid - 1000;

	for (new i = 0; i < ZVEH_MAX_COMPONENTS; i++) {
		if (gComponents[index]{i} == ZVEH_INVALID_COMPONENT_ID) {
			return 0;
		}

		if (gComponents[index]{i} == component_value) {
			return 1;
		}
	}

	return 0;
}

stock IsVehicleHaveUpgrades(model)
{
	if (!(400 <= model <= 611)) {
		return 0;
	}

	return gComponents[model - 400]{0} == ZVEH_INVALID_COMPONENT_ID;
}
